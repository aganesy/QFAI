{
  "name": "qfai",
  "version": "0.5.0",
  "type": "module",
  "bin": {
    "qfai": "./dist/cli/index.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "prepack": "npm run build",
    "lint": "eslint src",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:assets": "vitest run tests/assets"
  },
  "dependencies": {
    "@cucumber/gherkin": "^37.0.1",
    "@cucumber/messages": "^31.1.0",
    "fast-glob": "^3.3.2",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@types/node": "^18.19.41",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  }
}
